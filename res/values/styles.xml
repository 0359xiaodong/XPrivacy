<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="CustomTheme" parent="android:Theme">
        <item name="icon_edit">@drawable/edit_holo_dark</item>
        <item name="icon_granted">@drawable/granted_holo_dark</item>
        <item name="icon_help">@drawable/help_holo_dark</item>
        <item name="icon_info">@drawable/info_holo_dark</item>
        <item name="icon_internet">@drawable/info_holo_dark</item>
        <item name="icon_used_grayed">@drawable/used_grayed_holo_dark</item>
        <item name="icon_used">@drawable/used_holo_dark</item>
    </style>

    <style name="CustomTheme.Light" parent="android:Theme.Light">
        <item name="icon_edit">@drawable/edit_holo_light</item>
        <item name="icon_granted">@drawable/granted_holo_light</item>
        <item name="icon_help">@drawable/help_holo_light</item>
        <item name="icon_info">@drawable/info_holo_light</item>
        <item name="icon_internet">@drawable/info_holo_light</item>
        <item name="icon_used_grayed">@drawable/used_grayed_holo_light</item>
        <item name="icon_used">@drawable/used_holo_light</item>
    </style>

</resources>